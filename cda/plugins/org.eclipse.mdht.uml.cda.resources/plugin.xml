<plugin>
   <extension
         id="CDAProfiles"
         name="CDA Profiles"
         point="org.eclipse.gmf.runtime.emf.core.Pathmaps">
      <pathmap
            name="CDA_PROFILES"
            path="profiles"
            plugin="org.eclipse.mdht.uml.cda.resources"/>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="pathmap://CDA_PROFILES/"
            target="profiles/">
      </mapping>
   </extension>
  
</plugin>
