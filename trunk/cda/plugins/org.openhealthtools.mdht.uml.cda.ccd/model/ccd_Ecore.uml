      </generalization>
    </packagedElement>
    <packagedElement xmi:type="uml:Class" xmi:id="_NvRwcKPeEd6t_4WfM070fA" name="AdvanceDirectivesSection">
      <ownedRule xmi:id="_kF7pQrjuEd6Keo9EfYidMA" name="AdvanceDirectivesSection_templateId" constrainedElement="_NvRwcKPeEd6t_4WfM070fA">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_kF7pQ7juEd6Keo9EfYidMA">
