      <ownedRule xmi:id="_SCNSMs70Ed6i-KsPaj7Wvg" name="MySectionTitle" constrainedElement="_Ucv9baX0Ed6JRdOhEffuGA">
        <specification xmi:type="uml:OpaqueExpression" xmi:id="_SCNSM870Ed6i-KsPaj7Wvg">
          <language>OCL</language>
          <body>not self.title.oclIsUndefined()</body>
        </specification>
      </ownedRule>
      <ownedRule xmi:id="_SCNSNM70Ed6i-KsPaj7Wvg" name="MySectionMyActivity" constrainedElement="_Ucv9baX0Ed6JRdOhEffuGA">
