        </specification>
      </ownedRule>
      <generalization xmi:id="ContextStructure_Document_generalization" general="ContextStructure"/>
      <ownedAttribute xmi:id="Document-completionCode" name="completionCode">
        <ownedComment xmi:id="_uKQQsGWmEd6rssu1fb4kpw">
          <body> 
								&lt;p>A code depicting the completion status of a report.&lt;/p>
							</body>
        </ownedComment>
        <type xmi:type="uml:Interface" href="pathmap://HL7_DT_UV/DEFN=UV=DT=1.0.uml#CE"/>
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_uKObgmWmEd6rssu1fb4kpw" value="1"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_uKObgWWmEd6rssu1fb4kpw"/>
      </ownedAttribute>
      <ownedAttribute xmi:id="Document-storageCode" name="storageCode">
        <ownedComment xmi:id="_uKWXUWWmEd6rssu1fb4kpw">
          <body> 
								&lt;p>The storage status of a report.&lt;/p>
							</body>
        </ownedComment>
        <type xmi:type="uml:Interface" href="pathmap://HL7_DT_UV/DEFN=UV=DT=1.0.uml#CE"/>
        <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_uKVJMGWmEd6rssu1fb4kpw" value="1"/>
        <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_uKUiIWWmEd6rssu1fb4kpw"/>
