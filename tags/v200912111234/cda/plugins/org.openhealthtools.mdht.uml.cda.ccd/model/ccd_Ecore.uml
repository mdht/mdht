    <packagedElement xmi:type="uml:Class" xmi:id="_5hfTYKPbEd6t_4WfM070fA" name="MedicalEquipmentSection">
      <ownedComment xmi:id="_fMDbQKPcEd6t_4WfM070fA" annotatedElement="_5hfTYKPbEd6t_4WfM070fA">
        <body>&lt;p>&#xD;
