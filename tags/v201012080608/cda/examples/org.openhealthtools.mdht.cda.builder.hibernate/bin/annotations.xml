		</eclass>

		<eclass name="Act">
			<inheritance>TABLE_PER_CLASS</inheritance>
		</eclass>

		<eclass name="ActRelationship">
			<inheritance>TABLE_PER_CLASS</inheritance>
		</eclass>

		<eclass name="Entity">
			<inheritance>TABLE_PER_CLASS</inheritance>
		</eclass>

		<eclass name="Participation">
			<inheritance>TABLE_PER_CLASS</inheritance>
		</eclass>

		<eclass name="Role">
			<inheritance>TABLE_PER_CLASS</inheritance>
		</eclass>

		<eclass name="RoleLink">
			<inheritance>TABLE_PER_CLASS</inheritance>
		</eclass>


	</epackage>



</persistence-mapping>